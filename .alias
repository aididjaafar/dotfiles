# Easier navigation: .., ..., ...., ....., ~ and -
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~" # `cd` is probably faster to type though
alias -- -="cd -"

# Shortcuts
alias cl="clear"
alias d="cd ~/Documents/Dropbox"
alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"
alias p="cd ~/Projects"
alias g="git"
alias h="history"
alias j="jobs"

# Homebrew 
alias bwl="brew list"
alias bwd="brew doctor"
alias bwu="brew update"
alias bwup="brew upgrade"
alias bwo="brew outdated"
alias bwc="brew cleanup"
alias bwcc="brew cask cleanup"
alias bwcs="brew cask search"
